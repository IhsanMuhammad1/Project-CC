



function sumOfMultiples(upToNum) {
let sum = 0

    for(let i =0; i<upToNum.length; i++) {
        let tmp3 = i / 3
        let tmp5 = i / 5

        if(Number.isInteger(tmp 3) || Number.isInteger(tmp 5)) {
            sum = sum + i
        }
       
    }
    return sum;
}

const ans = sumOfMultiples(1000);

console.log(ans);